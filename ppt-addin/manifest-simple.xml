<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">

  <Id>12345678-abcd-1234-5678-123456789012</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Company</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Asset Library" />
  <Description DefaultValue="Access your company asset library" />

  <IconUrl DefaultValue="http://127.0.0.1:8090/assets/icon-32.png" />
  <HighResolutionIconUrl DefaultValue="http://127.0.0.1:8090/assets/icon-64.png" />

  <SupportUrl DefaultValue="http://127.0.0.1:8090/" />

  <AppDomains>
    <AppDomain>http://127.0.0.1:8090</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Presentation" />
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="http://127.0.0.1:8090/taskpane.html" />
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

</OfficeApp>